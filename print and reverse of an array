#include<stdio.h>
int main()
{
int arr[100],rev[100];
printf("Enter number of elements:");
scanf("%d",&n);
printf("Enter %d elements :\n",n);
for(i=0;i<n;i++)
{
scanf("%d",&arr[i]);
}
for(i=0;i<n;i++)
{
rev[i]=arr[n-i-1];
}
printf("Reversed of an array elements");
for(i=0;i<n;i++);
{
printf("%d",rev[i]);
}
return 0
}   



output: 
Enter number of elements: 5
Enter  5 elements:
10 20 30 40 50
Reversed of an array:
50 40 30 20 10
